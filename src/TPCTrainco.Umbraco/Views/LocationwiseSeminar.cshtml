@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "Base.cshtml";
    IPublishedContent currentPage = Model.Content;
    ViewBag.location = currentPage.GetPropertyValue<string>("location");
}
<div class="register-cart-wrap">
    <shopping-cart></shopping-cart>
</div>
<div class="container-fluid headline-section">
    <div class="row">
        <div class="jumbotron">
            <h1>@(currentPage.GetPropertyValue<string>("pageTitle", currentPage.Name))</h1>
        </div>
    </div>
</div>

<div class="container-fluid main-content offline-payment gradient-bg">
    <div class="container">
        <div class="col-md-12 text-wrap">
            <div class="page-content">
                @Html.Raw(currentPage.GetPropertyValue<string>("mainContent"))
            </div>
        </div>
        <div class="clearfix"></div>
        @Html.Partial("Seminars/LocationSeminars")
    </div>
</div>