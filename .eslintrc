{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "plugins": [
    "babel",
    "angular",
    "react"
  ],
    "globals": {
    "angular": true,
    "module": true,
    "inject": true
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "destructuring": true,
    "modules": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "templateStrings": true,
    "jsx": true,
    "restParams": true,
    "spread": true
  },
  "rules": {
    "strict": 0,
    "quotes": [2, "single"]
  }
}
